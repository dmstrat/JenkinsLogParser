  <ItemGroup>
    <PackageReference Include="CommandLineParser" Version="2.9.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\JenkinsLogParser\JenkinsLogParser.csproj" />
  </ItemGroup>

</Project>
